    {
        "id": "6d1cf10c.ff47",
        "type": "function",
        "z": "8bcf2d43.2e243",
        "name": "datetime",
        "func": "const format = (n, l) => String(n).padStart(l, '0')\nconst deviceId = 405419896\n\nswitch (msg.topic) {\n    case 'set-date':\n        flow.set('set-time.date', new Date(msg.payload))\n        return null;\n\n    case 'set-time':\n        const dt = new Date(msg.payload)\n        const tz = dt.getTimezoneOffset()\n        flow.set('set-time.time', new Date(msg.payload + tz*60*1000))\n        return null;\n        \n    case 'set':\n        let date = flow.get('set-time.date')\n        let time = flow.get('set-time.time')\n\n        if (!date) {\n          date = new Date()\n        }\n\n        if (!time) {\n          time = new Date()\n        }\n\n        const year = format(date.getFullYear(), 4)\n        const month = format(date.getMonth()+1, 2)\n        const day = format(date.getDate(), 2)\n        const hours = format(time.getHours(), 2)\n        const minutes = format(time.getMinutes(), 2)\n        const seconds = format(time.getSeconds(), 2)\n\n        msg.payload = { \n            deviceId: deviceId,\n            datetime: year + '-' + month + '-' + day + ' ' + hours + ':' + minutes + ':' + seconds\n        }\n        \n        return msg\n}",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "x": 480,
        "y": 320,
