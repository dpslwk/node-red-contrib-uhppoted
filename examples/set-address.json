    {
        "id": "45ee145d.a3525c",
        "type": "function",
        "z": "8bcf2d43.2e243",
        "name": "IPv4",
        "func": "const deviceId = 405419896\n\nswitch (msg.topic) {\n    case 'set-address':\n        flow.set('set-address.address', msg.payload)\n        return null;\n\n    case 'set-subnet':\n        flow.set('set-address.subnet', msg.payload)\n        return null;\n\n    case 'set-gateway':\n        flow.set('set-address.gateway', msg.payload)\n        return null;\n        \n    case 'set':\n        let address = flow.get('set-address.address')\n        let subnet = flow.get('set-address.subnet')\n        let gateway = flow.get('set-address.gateway')\n\n        if (!address) {\n          address = '192.168.1.100'\n        }\n\n        if (!subnet) {\n          subnet = '255.255.255.20'\n        }\n\n        if (!gateway) {\n          gateway = '192.168.1.1'\n        }\n\n        msg.payload = {\n           deviceId: deviceId,\n           address: address,\n           subnet: subnet,\n           gateway: gateway\n        }        \n        \n        return msg\n}\n\n",
